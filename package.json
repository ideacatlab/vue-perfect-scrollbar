{
    "name": "@razvanrun/vue3-perfect-scrollbar",
    "version": "0.0.1",
    "description": "Updated Mirror of Vue.js wrapper for perfect scrollbar",
    "paths": {
        "source": "src",
        "destination": "dist",
        "example": "example"
    },
    "main": "dist/vue3-perfect-scrollbar.cjs.js",
    "module": "dist/vue3-perfect-scrollbar.esm.js",
    "style": "dist/vue3-perfect-scrollbar.css",
    "types": "./types/vue3-perfect-scrollbar.d.ts",
    "scripts": {
        "build": "rollup -c --environment production",
        "build:css": "postcss src/style.css -o dist/vue3-perfect-scrollbar.min.css --env production && postcss src/style.css -o dist/vue3-perfect-scrollbar.css",
        "dev": "webpack-dev-server --config webpack.config.js",
        "test": "yarn jest",
        "test:dev": "node --inspect-brk node_modules/.bin/jest --runInBand"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/ideacatlab/vue-perfect-scrollbar.git"
    },
    "author": "Razvan Gheorghe <razvan@ideacat.ro>",
    "keywords": [
        "frontend",
        "scroll",
        "scrollbar",
        "vue"
    ],
    "license": "MIT",
    "devDependencies": {
        "@babel/core": "^7.23.2",
        "@babel/preset-env": "^7.23.2",
        "@rollup/plugin-commonjs": "^25.0.7",
        "@rollup/plugin-node-resolve": "^15.2.3",
        "@vue/compiler-sfc": "^3.3.7",
        "babel-loader": "^9.1.3",
        "css-loader": "^6.8.1",
        "postcss": "^8.4.31",
        "postcss-cli": "^10.1.0",
        "rollup": "^4.1.4",
        "sass-loader": "^13.3.2",
        "style-loader": "^3.3.3",
        "ts-loader": "^9.5.0",
        "typescript": "^5.2.2",
        "vue": "^3.3.7",
        "vue-loader": "^17.3.0",
        "vue-style-loader": "^4.1.3",
        "webpack": "^5.89.0",
        "webpack-cli": "^5.1.4"
    },
    "dependencies": {
        "cssnano": "^6.0.1",
        "perfect-scrollbar": "^1.5.5",
        "postcss-import": "^15.1.0"
    },
    "volta": {
        "node": "18.18.2"
    },
    "bugs": {
        "url": "https://github.com/ideacatlab/vue-perfect-scrollbar/issues"
    },
    "homepage": "https://github.com/ideacatlab/vue-perfect-scrollbar#readme",
    "directories": {
        "example": "example"
    }
}
